{
  "kind": "PersistentVolumeClaim",
  "apiVersion": "v1",
  "metadata": {
    "name": "spark-history-efs",
    "namespace": "spark-cluster",
    "labels": {
        "environment": "staging",
        "name": "spark-history-efs"
    }
  },
  "spec": {
    "selector": {
        "matchLabels": {
            "environment": "staging",
            "name": "spark-history-efs"
        }
    },
    "accessModes": [
      "ReadWriteMany"
    ],
    "storageClassName": "",
    "resources": {
      "requests": {
        "storage": "10Gi"
      }
    }
  }
}
